# Moltbook Integration Configuration

# ============================================
# API Configuration
# ============================================

# Moltbook API Key (Required)
MOLTBOOK_API_KEY=moltbook_sk_3j4CexZeIxD4sfw0F4S-oUsemCW3NiEX

# Base URL
MOLTBOOK_BASE_URL=https://www.moltbook.com/api/v1

# API Version
MOLTBOOK_VERSION=1

# ============================================
# Rate Limiting
# ============================================

# Requests per minute
MOLTBOOK_RATE_LIMIT=100

# Requests per window
MOLTBOOK_RATE_WINDOW=60000

# ============================================
# Timeout Configuration
# ============================================

# Request timeout (milliseconds)
MOLTBOOK_TIMEOUT=30000

# Retry count
MOLTBOOK_MAX_RETRIES=3

# Retry delay (milliseconds)
MOLTBOOK_RETRY_DELAY=1000

# ============================================
# Caching
# ============================================

# Cache TTL (seconds)
MOLTBOOK_CACHE_TTL=300

# Enable caching
MOLTBOOK_CACHE_ENABLED=true

# ============================================
# Logging
# ============================================

# Log level: debug, info, warn, error
MOLTBOOK_LOG_LEVEL=info

# Log file path
MOLTBOOK_LOG_FILE=./logs/moltbook.log
